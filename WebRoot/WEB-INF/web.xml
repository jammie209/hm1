<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>test01</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

  <servlet>
  <servlet-name>AccountAddServlet</servlet-name>
  <servlet-class>action.AccountAddServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AccountListServlet</servlet-name>
    <servlet-class>action.AccountListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AccountShowServlet</servlet-name>
    <servlet-class>action.AccountShowServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AccountUpdateServlet</servlet-name>
    <servlet-class>action.AccountUpdateServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AccountDeleteServlet</servlet-name>
    <servlet-class>action.AccountDeleteServlet</servlet-class>
  </servlet>







  <servlet-mapping>
  <servlet-name>AccountAddServlet</servlet-name>
  <url-pattern>/AccountAddServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AccountListServlet</servlet-name>
    <url-pattern>/AccountListServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AccountShowServlet</servlet-name>
    <url-pattern>/AccountShowServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AccountUpdateServlet</servlet-name>
    <url-pattern>/AccountUpdateServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AccountDeleteServlet</servlet-name>
    <url-pattern>/AccountDeleteServlet</url-pattern>
  </servlet-mapping>

</web-app>